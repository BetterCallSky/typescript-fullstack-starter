{
  "name": "sample-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev": "better-npm-run dev",
    "start": "better-npm-run start"
  },
  "author": "",
  "betterScripts": {
    "dev": {
      "command": "ts-node-dev --respawn --transpileOnly src/app.ts  | bunyan -o short -l trace",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev2": {
      "command": "nodemon --exec babel-node src/app.ts  -i asdsa  --extensions .ts | bunyan -o short -l trace",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "babel-node src/app.ts --extensions .ts",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@babel/node": "^7.2.0",
    "@babel/plugin-syntax-export-extensions": "^7.0.0-beta.32",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/bunyan": "^1.8.5",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.3.3",
    "@types/mz": "^0.0.32",
    "@types/passport": "^0.4.7",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/uuid": "^3.4.4",
    "better-npm-run": "^0.1.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "config": "^3.0.0",
    "cors": "^2.8.5",
    "defensive": "^0.0.3",
    "express": "^4.16.4",
    "mongoose": "^5.3.14",
    "mz": "^2.7.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "ts-mongoose": "^0.0.6",
    "ts-node-dev": "^1.0.0-pre.31",
    "uuid": "^3.3.2",
    "veni": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^1.10.2"
  }
}
