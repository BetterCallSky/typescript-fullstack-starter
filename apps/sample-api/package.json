{
  "name": "sample-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev": "better-npm-run dev",
    "start": "better-npm-run start"
  },
  "author": "",
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node src/app.ts --extensions .ts | bunyan -o short -l trace",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "babel-node src/app.ts --extensions .ts",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@babel/node": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/bunyan": "^1.8.5",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.3.3",
    "better-npm-run": "^0.1.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "config": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "mongoose": "^5.3.14",
    "ts-mongoose": "^0.0.6",
    "veni": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^1.10.2"
  }
}
